{
  "compilerOptions": {
    "target": "es6",
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": ["esnext"],
    "composite": true,
    "outDir": "build_output",
    "sourceMap": true
  },
  "include": ["src"]
}
