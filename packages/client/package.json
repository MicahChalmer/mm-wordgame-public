{
  "name": "mm-wordgame-client",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.1.40",
    "aws-amplify-react": "^2.4.4",
    "fast-memoize": "^2.5.1",
    "formik": "^1.5.8",
    "immer": "^4.0.0",
    "mm-wordgame-common": "0.0.0",
    "pretty-format": "^24.9.0",
    "react": "^16.9.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-loading": "^2.0.3",
    "react-modal": "^3.10.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.2",
    "react-select": "^3.0.5",
    "shuffle-array": "^1.0.1",
    "ts-union": "^2.1.1"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "full-build": "tsc -b && rescripts build"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2019,
      "project": "./tsconfig.json"
    },
    "extends": [
      "../../eslintConfig.defaults.js",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "overrides": [
      {
        "files": [
          "src/serviceWorker.js"
        ],
        "rules": {
          "@typescript-eslint/no-use-before-define": "off"
        }
      }
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@rescripts/cli": "0.0.12",
    "@rescripts/rescript-use-eslint-config": "^0.0.9",
    "@testing-library/react": "^9.1.4",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-autosuggest": "^9.3.11",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-dom": "^16.9.0",
    "@types/react-modal": "^3.8.3",
    "@types/react-router": "^5.0.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-select": "^3.0.4",
    "@types/resolve": "^0.0.8",
    "@types/shuffle-array": "0.0.28",
    "@types/zen-observable": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "jest-fetch-mock": "2.1.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "resolve": "^1.12.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "proxy": "http://localhost:4000/"
}
