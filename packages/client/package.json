{
  "name": "mm-wordgame-client",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.2.4",
    "aws-amplify-react": "^2.5.4",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.0.3",
    "fast-memoize": "^2.5.1",
    "formik": "^2.0.3",
    "immer": "^9.0.6",
    "mm-wordgame-common": "0.0.0",
    "pretty-format": "^24.9.0",
    "react": "^16.11.0",
    "react-beautiful-dnd": "^12.0.0",
    "react-dom": "^16.11.0",
    "react-loading": "^2.0.3",
    "react-modal": "^3.11.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "react-select": "^3.0.8",
    "shuffle-array": "^1.0.1",
    "ts-union": "^2.1.1"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "full-build": "tsc -b && rescripts build"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2019,
      "project": "./tsconfig.json"
    },
    "extends": [
      "../../eslintConfig.defaults.js",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "overrides": [
      {
        "files": [
          "src/serviceWorker.js"
        ],
        "rules": {
          "@typescript-eslint/no-use-before-define": "off",
          "@typescript-eslint/explicit-function-return-type": "off"
        }
      }
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@rescripts/cli": "0.0.13",
    "@rescripts/rescript-use-eslint-config": "^0.0.10",
    "@testing-library/react": "^9.3.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.22",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-autosuggest": "^9.3.13",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-dom": "^16.9.4",
    "@types/react-modal": "^3.10.0",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-select": "^3.0.8",
    "@types/resolve": "^0.0.8",
    "@types/shuffle-array": "0.0.28",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.4",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "jest-fetch-mock": "2.1.2",
    "prettier": "^1.19.0",
    "pretty-quick": "^2.0.1",
    "rescripts-use-yarn-workspaces": "^1.1.0",
    "resolve": "^1.12.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "rescripts": [
    [
      "use-eslint-config",
      "package"
    ],
    "rescripts-use-yarn-workspaces"
  ]
}
